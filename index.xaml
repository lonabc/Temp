<Window x:Class="WpfAppLogin.index"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppLogin"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        mc:Ignorable="d"
        Title="index" Height="450" Width="800">
 
    <StackPanel>

        <Label Margin="10">原生控件</Label>
        <Button Content="普通按钮" Width="100" Height="40" />
        <!-- 使用 HandyControl 的控件，例如按钮1 -->
        <Button Content="测试"  Style="{StaticResource ButtonDashedInfo}"   Margin="5"/>
        <Button Content="wpf项目测试" Style="{StaticResource ButtonPrimary}"/>
        <Button Content="项目测试完成"/>
    </StackPanel>
</Window>
